<Page x:Class="Zoopark.EmplyeeTypePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Zoopark"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EmplyeeTypePage">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="btn_admin" Content="ADMIN" Grid.Row="1" Grid.ColumnSpan="3" HorizontalAlignment="Center" Width="300" Height="60" Background="#FFA2F09A" BorderBrush="Black" Click="btn_admin_Click"/>
        <Button x:Name="btn_user" Content="USER" Grid.Row="2" Grid.ColumnSpan="3" HorizontalAlignment="Center" Width="300" Height="60" Background="#FFA2F09A" BorderBrush="Black" Click="btn_user_Click"/>
    </Grid>
</Page>
